<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="CircleProgressView">
        <attr name="circleColor" format="color"/>
        <attr name="circleProgressColor" format="color"/>
        <attr name="circleWidth" format="dimension"/>
        <attr name="circleTextColor" format="color"/>
        <attr name="circleTextSize" format="dimension"/>
        <attr name="circleStartAngle" format="integer"/>
        <attr name="circleAngle" format="integer"/>
    </declare-styleable>

    <declare-styleable name="HorizontalProgressView">
        <attr name="horizontalColor" format="color" />
        <attr name="horizontalProgressColor" format="color" />
    </declare-styleable>
    
    <declare-styleable name="SwitchButton">
        <attr name="selectColor" format="color"/>
        <attr name="normalColor" format="color"/>
        <attr name="buttonColor" format="color"/>
        <attr name="buttonPadding" format="dimension" />
        <attr name="switchRate" format="integer" />
    </declare-styleable>
</resources>